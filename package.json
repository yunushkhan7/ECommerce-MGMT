{
  "name": "ecommercemgmt",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration=production --optimization=false",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:stats": "ng build --stats-json",
    "analyzer": "webpack-bundle-analyzer dist/ecommercemgmt/stats.json",
    "analyze": "npm run build:stats && npm run analyzer",
    "deploy": "git pull origin dev && ng build && firebase deploy"
  },
  "private": true,
